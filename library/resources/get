get() {

# these describe the options that we want the get resource to have.
# on a successful run, the parse funtion will remove all values that are specified
  required_inputs=(
'ORIGIN="http remote origin of a file"'
'MD5="MD5 sum of remote file"'
)

# parse command line inputs 
parse_inputs $@

# make sure that all required params were set
if ! test "${required_inputs[*]}" == ' ' ; then 
    echo all required inputs were not found. returning 1
    return 1
fi



exit 0 

  if [[ -z $SHEEP_DEVELOP ]] ; then 
      CURL_ARGS=' --silent '
  fi
  
  echo curling \"curl --location --output ${CACHE_DIR}/${MD5} $ORIGIN   \"
  if test -f ${CACHE_DIR}/${MD5} ; then 
      debug "found file in local cache"
  elif $( curl -o ${CACHE_DIR}/${MD5} $ORIGIN ) ; then 
      debug "downloaded remote file"
    else 
      echo could not get remote file
      return 1
  fi
  
  # verify md5 of file in cache
  verify_cached_file $MD5
  
}
